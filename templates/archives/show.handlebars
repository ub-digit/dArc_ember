<div class="container-fluid">
	<div class="page-head">
		<div class="row v-align">
			<div class="col-xs-3">
				{{#link-to 'archives'}}<i class="fa fa-chevron-left"></i> <span class="back-title">{{translateString STRINGS.PAGES.ARCHIVES.TITLE}}</span>{{/link-to}}
			</div>
			<div class="col-xs-9">
				<h1>{{translateString STRINGS.PAGES.ARCHIVE.TITLE}} </h1>
			</div>
		</div> <!-- end row --> 
	</div> <!-- end page-head --> 
	<div class="row">
		<div class="col-xs-12">
			<table class="table table-striped">
				<tr>
					<th>id:</th>
					<td>{{model.id}}</td>
				</tr>
				<tr>
					<th>pid:</th>
					<td>{{model.pid}}</td>
				</tr>
				<tr>
					<th>Title:</th>
					<td>{{model.title}}</td>
				</tr>
				<tr>
					<th>Unittitle:</th>
					<td>{{model.unittitle}}</td>
				</tr>
				<tr>
					<th>Unitdate:</th>
					<td>{{model.unitdate}}</td>
				</tr>
				<tr>
					<th>Unitid:</th>
					<td>{{model.unitid}}</td>
				</tr>
				<tr>
					<th>Abstract:</th>
					<td>{{model.abstract}}</td>
				</tr>
				<tr>
					<th>Authorities:</th>
					<td>
						{{#each model.authorities }}{{title}}, {{/each}}
					</td>
				</tr>							
			</table>
		</div>
	</div><!-- end row --> 
</div> <!-- end container --> 