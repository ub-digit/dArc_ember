<div class="container-fluid">
	<div class="row">
		<div class="col-xs-12">
			<h3>List of archives</h3>
			<ul>
				{{#each item in model}}
					<li>{{#link-to 'archives.show' item.id}}id: {{item.id}}, exampleStr: {{item.exampleStr}}, exampleBool: {{item.exampleBool}}, exampleDate: {{item.exampleDate}}{{/link-to}}</li>
				{{/each}}
			</ul>
		</div>
	</div>
</div>